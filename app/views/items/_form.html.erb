<%= twitter_bootstrap_form_for @item do |f| %>
  <%= f.inputs 'Create' do %>
    <%= f.text_field :name, class: 'span7' %>
    <%= f.text_field :tags, class: 'span7 autocomplete_with_tags', 'data-pre' => f.object.prepopulate_tags.to_json %>
    <%= f.file_field :image %>

    <%= f.actions do %>
      <%= f.submit %>
      <%= button_tag  'Cancel', type: 'reset', class: 'btn' %>
    <% end %>
  <% end %>
<% end %>
